{
  "manifest_version": 2,
  "name": "Chrome Extension Starter Kit",
  "description": "",
  "version": "1.0.0",
  "icons": {
    "16": "icon/icon-16.png",
    "32": "icon/icon-32.png",
    "48": "icon/icon-48.png",
    "128": "icon/icon-128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icon/icon-19.png",
      "38": "icon/icon-38.png"
    },
    "default_popup": "html/popup.html",
    "default_title": "Chrome Extension Starter Kit"
  },
  "options_ui": {
    "page": "html/options.html",
    "chrome_style": true
  },
  "background": {
    "scripts": [
      "js/config.js",
      "js/fn.js",
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://www2.ucsc.edu/courses/cse112-wm/*"
      ],
      "js": [
        "js/config.js",
        "js/fn.js",
        "js/content.js"
      ],
      "css": [
        "css/content.css"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs",
    "contextMenus",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "web_accessible_resources": ["css/bluma.css"]
}
